<h2>Available drones</h2>

{{#each drones}}
<div>
    <a href="/drones/{{_id}}">Name:{{name}}</a>
    <p>Propellers: {{propellers}}</p>
    <p>Speed: {{maxSpeed}}</p>
    <form action="/drones/{{_id}}/delete" method="post">
    <button type="submit">Delete</button>
    </form>
    <a href="/drones/{{this._id}}/edit">Edit</a>
</div>
{{/each}}